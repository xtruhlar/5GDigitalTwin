            "source": [
                "import unittest\n",
                "from unittest.mock import patch, MagicMock\n",
                "import pandas as pd\n",
                "from datetime import datetime, timedelta\n",
                "import pytz\n",
                "from data.main import prom, metrics, STEP, LOCAL_TZ, start_time, end_time\n"
            ]
        },
        {
            "cell_type": "code",
            "metadata": {},
            "source": [
                "class TestPrometheusDataFetching(unittest.TestCase):\n",
                "    @patch('data.main.prom.custom_query_range')\n",
                "    def test_valid_data(self, mock_query):\n",
                "        # Mock response with valid data\n",
                "        mock_query.return_value = [\n",
                "            {\n",
                "                \"metric\": {\"__name__\": \"test_metric\"},\n",
                "                \"values\": [[str(int(datetime.now().timestamp())), \"123.45\"]]\n",
                "            }\n",
                "        ]\n",
                "\n",
                "        # Execute the code\n",
                "        df_list = []\n",
                "        for group, metric_list in metrics.items():\n",
                "            for metric in metric_list:\n",
                "                response = prom.custom_query_range(\n",
                "                    metric, start_time=start_time, end_time=end_time, step=STEP\n",
                "                )\n",
                "                for entry in response:\n",
                "                    base_metric_name = entry[\"metric\"].get(\"__name__\", metric)\n",
                "                    extracted_values = [\n",
                "                        (\n",
                "                            datetime.utcfromtimestamp(int(ts))\n",
                "                            .replace(tzinfo=pytz.utc)\n",
                "                            .astimezone(LOCAL_TZ),\n",
                "                            float(val)\n",
                "                        )\n",
                "                        for ts, val in entry[\"values\"]\n",
                "                    ]\n",
                "                    metric_df = pd.DataFrame(extracted_values, columns=[\"timestamp\", \"value\"])\n",
                "                    metric_df[\"metric_name\"] = base_metric_name\n",
                "                    metric_df[\"group\"] = group\n",
                "                    df_list.append(metric_df)\n",
                "\n",
                "        final_df = pd.concat(df_list, ignore_index=True)\n",
                "        self.assertFalse(final_df.empty)\n",
                "        self.assertIn(\"test_metric\", final_df[\"metric_name\"].values)\n",
                "\n",
                "    @patch('data.main.prom.custom_query_range')\n",
                "    def test_no_data(self, mock_query):\n",
                "        # Mock response with no data\n",
                "        mock_query.return_value = []\n",
                "\n",
                "        # Execute the code\n",
                "        df_list = []\n",
                "        for group, metric_list in metrics.items():\n",
                "            for metric in metric_list:\n",
                "                response = prom.custom_query_range(\n",
                "                    metric, start_time=start_time, end_time=end_time, step=STEP\n",
                "                )\n",
                "                if not response:\n",
                "                    empty_df = pd.DataFrame({\n",
                "                        \"timestamp\": pd.date_range(start=start_time, end=end_time, freq=STEP),\n",
                "                        \"value\": [float(\"nan\")] * len(pd.date_range(start=start_time, end=end_time, freq=STEP)),\n",
                "                        \"metric_name\": metric,\n",
                "                        \"group\": group\n",
                "                    })\n",
                "                    df_list.append(empty_df)\n",
                "\n",
                "        final_df = pd.concat(df_list, ignore_index=True)\n",
                "        self.assertFalse(final_df.empty)\n",
                "        self.assertTrue(final_df[\"value\"].isna().all())\n",
                "\n",
                "    @patch('data.main.prom.custom_query_range')\n",
                "    def test_exception_handling(self, mock_query):\n",
                "        # Mock exception during query\n",
                "        mock_query.side_effect = Exception(\"Test exception\")\n",
                "\n",
                "        # Execute the code\n",
                "        df_list = []\n",
                "        for group, metric_list in metrics.items():\n",
                "            for metric in metric_list:\n",
                "                try:\n",
                "                    prom.custom_query_range(\n",
                "                        metric, start_time=start_time, end_time=end_time, step=STEP\n",
                "                    )\n",
                "                except Exception as e:\n",
                "                    self.assertEqual(str(e), \"Test exception\")\n",
                "\n",
                "        self.assertEqual(len(df_list), 0)\n",
                "\n",
                "if __name__ == '__main__':\n",
                "    unittest.main(argv=[''], exit=False)\n"
            ]
        }
    ]
}